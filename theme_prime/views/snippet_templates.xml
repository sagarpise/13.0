    <!-- Generic Options -->
    <template id="s_options" inherit_id="website.snippet_options">
        <xpath expr="//div[@data-js='parallax']" position="attributes">
            <attribute name="data-exclude" add=".s_no_parallax_speed"/>
        </xpath>
        <xpath expr="//div[@data-js='menu_data']" position="attributes">
            <attribute name="data-selector">#top_menu > li > a</attribute>
        </xpath>
        <xpath expr="//div[@data-js='background']" position="before">
            <div data-js="droggol_product_snippet" data-selector=".droggol_product_snippet">
                <we-button data-set-grid="true" data-no-preview="true">
                    <i class="fa fa-shopping-cart fa-fw"/> Manage Products
                </we-button>
            </div>
            <div data-js="droggol_category_mega_menu_snippet" data-selector=".droggol_category_mega_menu_snippet">
                <we-button data-set-category="true" data-no-preview="true">
                    <i class="fa fa-shopping-cart fa-fw"/> Manage Categories
                </we-button>
            </div>
            <div data-js="droggol_gradient_option" data-selector="section, :not(.o_gallery > .container) > .carousel, .d_enable_bg_img">
                <we-button data-set-gradient="true" data-no-preview="true">
                    <i class="fa fa-adjust fa-fw"/> Background Gradiant
                </we-button>
            </div>
        </xpath>
    </template>

</odoo>
